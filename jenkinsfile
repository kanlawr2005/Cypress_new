pipeline {
    agent any {
        
    stages {
        stage('Install Dependencies') { 
            steps {
                sh 'npm install cypress'
                sh 'npx cypress run'
            }
        }
        stage('Build') { 
            steps {
                sh 'npm run build'
            }
        }
        stage('Test') { 
            steps {
                sh 'npm run ci:cy-run'
            }
        }
    }
}
}


